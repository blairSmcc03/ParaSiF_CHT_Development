FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0/fluid";
    object      p_rgh;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 1 -1 -2 0 0 0 0 ];

internalField   uniform 0;

boundaryField
{
    topWall
    {
        type            fixedFluxPressure;
        value           $internalField;
    }
    leftWall
    {
        type            fixedFluxPressure;
        value           $internalField;
    }
    rightWall
    {
        type            fixedFluxPressure;
        value           $internalField;
    }
    bottomWall
    {
        type            fixedFluxPressure;
        value           $internalField;
    }

    fluid_to_solid
    {   
        type            fixedFluxPressure;
        value           $internalField;
    }
    frontAndBack
    {
        type            empty;
    }
}

// ************************************************************************* //
